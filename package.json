{
  "name": "667-hail-to-the-proxy-poker-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development DEBUG=APP_NAME:* nodemon ./bin/www",
    "start:dev:win": "SET \"NODE_ENV=development\" SET DEBUG=myapp:* & nodemon ./bin/www",
    "db:create:migration": "npx sequelize migration:generate --name ",
    "db:migrate": "npx sequelize db:migrate",
    "db:rollback": "npx sequelize db:migrate:undo",
    "db:dropAll": "npx sequelize db:migrate:undo:all",
    "postinstall": "npx sequelize db:migrate"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg-promise": "^10.12.0",
    "sequelize": "^6.25.0",
    "sequelize-cli": "^6.5.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
